{
    "name": "tracked",
    "version": "1.0.0",
    "description": "Application for tracking movies",
    "main": "app.js",
    "private": true,
    "scripts": {
        "prod": "tsc -p . && npm run build_ && npm run copy && node dist/server/src/app.js",
        "server": "nodemon src/app.ts",
        "client": "npm run start --prefix ../client",
        "build_": "npm run build --prefix ../client",
        "copy": "ncp ../client/dist dist/client/dist",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "eslint": "eslint . ",
        "eslint:fix": "eslint . --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codyfet/tracked.git"
    },
    "keywords": [
        "tmdb",
        "react",
        "redux",
        "express",
        "semantic-ui-react",
        "mongodb"
    ],
    "author": "codyfet <codyfet@mail.ru>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/codyfet/tracked/issues"
    },
    "homepage": "https://github.com/codyfet/tracked#readme",
    "dependencies": {
        "@types/config": "0.0.39",
        "async": "^3.2.0",
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "express-validator": "^6.5.0",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "mongoose": "^5.13.5",
        "morgan": "^1.10.0",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0"
    },
    "devDependencies": {
        "@types/async": "^3.2.7",
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/lodash": "^4.14.171",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.7.2",
        "@types/passport": "^1.0.7",
        "@types/passport-jwt": "^3.0.6",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "concurrently": "^5.2.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "ncp": "^2.0.0",
        "nodemon": "^2.0.4",
        "prettier": "^2.3.2",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    }
}
